package listDemo;
import java.util.ArrayList;

public class TestArrayList {

	public static void main(String[] args) {
		
		ArrayList flowers=new ArrayList();
		flowers.add("tulips");
		flowers.add("rose");
		flowers.add("jasmine");
		flowers.add("cosmos");
		flowers.add("lilly");
		System.out.println(flowers);
		System.out.println(flowers.size());
		
	}

}
